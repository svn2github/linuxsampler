 <class>qsamplerInstrumentListForm</class>
 <widget class="QMainWindow" name="qsamplerInstrumentListForm">
  <property name="geometry">
   <rect>
    <x>0</x>
    <y>0</y>
    <width>720</width>
    <height>340</height>
   </rect>
  </property>
  <property name="windowTitle">
   <string>Qsampler: Instruments</string>
  </property>
  <property name="windowIcon">
   <iconset resource="qsampler.qrc">:/images/qsamplerInstrument.png</iconset>
  </property>
  <widget class="QWidget" name="centralWidget" />
  <widget class="QToolBar" name="instrumentToolbar">
   <property name="orientation">
    <enum>Qt::Horizontal</enum>
   </property>
   <attribute name="toolBarArea">
    <enum>TopToolBarArea</enum>
   </attribute>
   <attribute name="toolBarBreak">
    <bool>false</bool>
   </attribute>
  </widget>
  <action name="newInstrumentAction">
   <property name="icon">
	<iconset resource="qsampler.qrc">:/images/itemNew.png</iconset>
   </property>
   <property name="text">
    <string>New &amp;Instrument...</string>
   </property>
   <property name="iconText">
    <string>New</string>
   </property>
   <property name="shortcut">
    <string>Ins</string>
   </property>
  </action>
  <action name="editInstrumentAction">
   <property name="icon">
	<iconset resource="qsampler.qrc">:/images/formEdit.png</iconset>
   </property>
   <property name="text">
    <string>&amp;Edit...</string>
   </property>
   <property name="iconText">
    <string>Edit</string>
   </property>
   <property name="shortcut">
    <string>Enter</string>
   </property>
  </action>
  <action name="deleteInstrumentAction">
   <property name="icon">
	<iconset resource="qsampler.qrc">:/images/formRemove.png</iconset>
   </property>
   <property name="text">
    <string>&amp;Delete</string>
   </property>
   <property name="iconText">
    <string>Delete</string>
   </property>
   <property name="shortcut">
    <string>Del</string>
   </property>
  </action>
  <action name="refreshInstrumentsAction">
   <property name="icon">
	<iconset resource="qsampler.qrc">:/images/formRefresh.png</iconset>
   </property>
   <property name="text">
    <string>&amp;Refresh</string>
   </property>
   <property name="iconText">
    <string>Refresh</string>
   </property>
   <property name="shortcut">
    <string>F5</string>
   </property>
  </action>
 </widget>
 <layoutdefault spacing="0" margin="0"/>
 <resources>
  <include location="qsampler.qrc"/>
 </resources>
 <connections/>
</ui>
