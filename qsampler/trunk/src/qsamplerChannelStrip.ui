    </connection>
    <connection>
        <sender>VolumeSlider</sender>
        <signal>valueChanged(int)</signal>
        <receiver>qsamplerChannelStrip</receiver>
        <slot>volumeChanged(int)</slot>
    </connection>
    <connection>
        <sender>VolumeSpinBox</sender>
        <signal>valueChanged(int)</signal>
        <receiver>qsamplerChannelStrip</receiver>
        <slot>volumeChanged(int)</slot>
    </connection>
</connections>
